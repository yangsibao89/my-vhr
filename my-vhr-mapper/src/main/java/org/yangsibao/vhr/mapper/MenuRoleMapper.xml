    <update id="updateByPrimaryKeySelective" parameterType="org.yangsibao.vhr.model.MenuRole">
        update menu_role
        <set>
            <if test="mid != null">
                mid = #{mid,jdbcType=INTEGER},
            </if>
            <if test="rid != null">
                rid = #{rid,jdbcType=INTEGER},
            </if>
        </set>
